<?xml version="1.0" encoding="UTF-8" ?>
<sourcemonitor_commands> 
   <write_log>true</write_log> 
   <log_all_to_console/>
   <command>
       <project_file>@PROJECT_FILE@</project_file>
       <file_extensions>*.cs,*.xaml|*.Designer.cs</file_extensions>
       <source_directory>@SOURCE_DIR@</source_directory>
       <include_subdirectories>true</include_subdirectories>
	   <source_subdirectory_list>
		<exclude_subdirectories>true</exclude_subdirectories>
		<source_subtree>Twice\bin\</source_subtree>
		<source_subtree>Twice\obj\</source_subtree>
		<source_subtree>ThirdParty\</source_subtree>
		<source_subtree>packages\</source_subtree>
	   </source_subdirectory_list>
       <checkpoint_name>@CHECKPOINT_NAME@</checkpoint_name>
   </command> 
</sourcemonitor_commands>